{
  "name": "vignan-telecalling-system",
  "version": "1.0.0",
  "description": "Vignan University Admissions Telecalling System",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "cd server && npm run dev",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "backend": "cd server && npm run dev",
    "frontend": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "server-install": "npm install --prefix server",
    "install-all": "npm install && npm run client-install && npm run server-install"
  },
  "keywords": [
    "telecalling",
    "admissions",
    "university"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}